{
  "name": "@rafaeljesus/events-worker",
  "version": "0.1.0",
  "description": "Record any actions your system perform",
  "scripts": {
    "start": "babel-node index.js",
    "clean": "rm -rf build && mkdir build",
    "build": "npm run clean && babel --presets es2015,stage-2 --plugins transform-runtime --ignore node_modules,test . -d build",
    "serve": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 build/index.js",
    "test": " npm run lint && NODE_ENV=test mocha",
    "lint": "standard --verbose | snazzy"
  },
  "author": "Rafael Jesus <rafaelljesus86@gmail.com> (http://github.com/rafaeljesus)",
  "license": "MIT",
  "pre-commit": [
  ],
  "standard": {
    "parser": "babel-eslint"
  },
  "ava": {
    "files": [
      "test/*spec.js"
    ],
    "failFast": true,
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "estraverse-fb": "^1.3.1",
    "pre-commit": "^1.1.2",
    "snazzy": "^3.0.0",
    "standard": "^6.0.7"
  },
  "dependencies": {
    "@rafaeljesus/events-model": "github:rafaeljesus/events-model",
    "wascally": "^0.2.10"
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
